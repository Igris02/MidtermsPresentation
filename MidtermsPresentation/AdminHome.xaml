<Window x:Class="MidtermsPresentation.AdminHome"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MidtermsPresentation"
        mc:Ignorable="d"
        Title="AdminHome" Height="450" Width="800">
    <Grid>
        <TextBlock HorizontalAlignment="Center" TextWrapping="Wrap" Text="Admin Management" VerticalAlignment="Top" FontWeight="Bold" Height="58" Width="396" FontSize="40" Margin="0,145,0,0"/>
        <Button x:Name="MUABtn" Content="Manage User Accounts" HorizontalAlignment="Center" Margin="0,266,0,0" VerticalAlignment="Top" Height="62" Width="232" FontSize="20" FontWeight="Bold" Click="MUABtn_Click"/>
        <Button x:Name="LGBtn" Content="Log Out" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="46" Width="119" FontWeight="Bold" FontSize="20"/>

    </Grid>
</Window>
